package praktikum3_tugas;

/**
 *
 * @author ASUS
 */
public class Main {
    public static void main(String[] args) { 
        Mobil mobil1 = new Mobil("Toyota", "Inova Zenix", 2024, "Putih");
        Mobil mobil2 = new Mobil("Honda", "Civic type-R", 2018, "Merah");
        
        mobil1.setWarna("Hitam"); 
        mobil1.displayinfo();
        mobil1.startEngine();
        
        mobil2.setWarna("Putih");
        mobil2.displayinfo();
        mobil2.startEngine();
    }   
}
